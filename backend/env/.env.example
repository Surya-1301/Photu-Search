# Example environment variables for backend/server.js
# Copy this file to ../.env or set these variables in your deployment environment.

# Server port (optional)
PORT=10000

# CORS origin for production (optional)
CORS_ORIGIN=https://your-production-site.example

# API keys for providers (replace with your real keys)
# Pexels (free developer key)
PEXELS_API_KEY=your_pexels_api_key_here

# Pixabay / Unsplash keys (optional)
PIXABAY_API_KEY=your_pixabay_api_key_here
UNSPLASH_ACCESS_KEY=your_unsplash_access_key_here

# Owner/admin tokens (optional)
OWNER_EMAIL=owner@example.com
ADMIN_TOKEN=some_admin_token_value

# Google OAuth (removed from this project) - left for historical reference only
# GOOGLE_CLIENT_ID=your_google_client_id

